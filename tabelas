CREATE TABLE aluno (
    id_aluno INT NOT NULL,
    nome_aluno VARCHAR(255) NOT NULL,
    cpf int NOT NULL,
    nome_mae VARCHAR(255) NOT NULL,
    nome_pai VARCHAR(255) NOT NULL,
    contato int,
    cep int,
    endereco VARCHAR(255),
    bairro VARCHAR (255),
    municipio VARCHAR(255),
    estado VARCHAR(255),
    data_nascimento DATE,
    genero VARCHAR (255),
    curso VARCHAR (255),
    deficiencia VARCHAR(255),
    escola VARCHAR (255),
    concorrencia VARCHAR (255),
    matematica_06 int,
    matematica_07 int,
    matematica_08 int,
    matematica_09 int,
    portugues_06 int,
    portugues_07 int,
    portugues_08 int,
    portugues_09 int,
    ciencia_06 int,
    ciencia_07 int,
    ciencia_08 int,
    ciencia_09 int,
    historia_06 int,
    historia_07 int,
    historia_08 int,
    historia_09 int,
    geografia_06 int,
    geografia_07 int,
    geografia_08 int,
    geografia_09 int,
    lingua_estrangeira_06 int,
    lingua_estrangeira_07 int,
    lingua_estrangeira_08 int,
    lingua_estrangeira_09 int,
    ensino_religioso_06 int,
    ensino_religioso_07 int,
    ensino_religioso_08 int,
    ensino_religioso_09 int,
    artes_06 int,
    artes_07 int,
    artes_08 int,
    artes_09 int,
    edf_06 int,
    edf_07 int,
    edf_08 int,
    edf_09 int,
);
